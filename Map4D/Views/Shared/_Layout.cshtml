<!DOCTYPE html>
<html lang="vi">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <META Http-Equiv="Cache-Control" Content="no-cache" />
    <META Http-Equiv="Pragma" Content="no-cache" />
    <META Http-Equiv="Expires" Content="0" />
    <title>@ViewBag.Title - Map4D</title>
    <link href="~/Content/Client/img/logoMap4D.png" type="image/png" rel="shortcut icon" />
    <link href="~/Content/Client/bootstrap4.1.0/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Admin/bower_components/font-awesome/css/all.css" rel="stylesheet" />
    <link href="~/Content/Client/css/styleMenu.css" rel="stylesheet" />
    <link href="~/Content/Client/css/styleFooter.css" rel="stylesheet" />
    @RenderSection("css", required: false)
</head>
<body>
    @Html.Partial("_Menu")
    <div class="row" id="wrapper">
        @RenderBody()
    </div>
    @Html.Partial("_Footer")
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Content/Client/bootstrap4.1.0/js/bootstrap.min.js"></script>
    <script src="~/Scripts/js/sticky-menu.js"></script>
    <script src="~/Scripts/js/footerScroll.js"></script>
    <script src="~/Scripts/sweetalert.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
