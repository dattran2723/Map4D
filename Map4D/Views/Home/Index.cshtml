@model Map4D.Models.Contact
@{
    /**/

    ViewBag.Title = @Map4D.Resources.My_texts.TrangChu;
}
@section css{
    <link href="~/Content/Client/css/styleSlideHome.css" rel="stylesheet" />
    <link href="~/Content/Client/css/styleMapbox.css" rel="stylesheet" />
    <link href="~/Content/Client/css/3DTrucQuan.css" rel="stylesheet" />
    <link href="~/Content/Client/css/CreateMobileApps.css" rel="stylesheet" />
    <link href="~/Content/Client/css/DirectionThereFaster.css" rel="stylesheet" />
    <link href="~/Content/Client/css/styleWebAppRelity.css" rel="stylesheet" />
    <link href="~/Content/Client/css/BuildMap4D.css" rel="stylesheet" />
    <link href="~/Content/Client/css/styleGetInTouch.css" rel="stylesheet" />
    <link href="~/Content/sweetalert/css/sweetalert.min.css" rel="stylesheet" />

}
@RenderPage("Slide.cshtml")
@RenderPage("Mapbox.cshtml")
@RenderPage("3DTrucQuan.cshtml")
@RenderPage("CreateMobileApps.cshtml")
@RenderPage("DirectionGetThereFaster.cshtml")
@RenderPage("WebAppRelity.cshtml")
@RenderPage("BuildWithMap4D.cshtml")
@RenderPage("GetInTouch.cshtml")
@section scripts{
    <script src="~/Content/sweetalert/js/sweetalert.js"></script>
    <script src="~/Scripts/js/hover-mapbox.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    @{
        var message = ViewBag.Message;
        if (message != null)
        {
            <script>
                $(document).ready(function () {
                    swal({
                        title: "@Html.Raw(Map4D.Resources.My_texts.Camongopy)",
                        type: "success",
                        confirmButtonClass: "btn-primary",
                        confirmButtonText: "OK",
                        closeOnConfirm: false
                    }, function () {
                        var url = '@Url.Action("Index")';
                        window.location.href = url.replace();
                    });
                });
            </script>
        }

    }
    @*js add content for view Build Map4D*@
    <script src="~/Scripts/js/addContentBuildMap4D.js"></script>
    <script src="~/Scripts/js/GetInTouch.js"></script>
}